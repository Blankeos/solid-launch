{
  "name": "solid-launch",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite --host",
    "build": "tsc && vite build",
    "preview": "NODE_ENV=production bun run src/server/server.ts",
    "preview-static": "vite preview",
    "lint": "prettier --check . && eslint .",
