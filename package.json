    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/client": "^5.15.1",
    "@tanstack/solid-query": "^5.45.0",
    "@trpc/client": "^11.0.0-rc.390",
    "@trpc/server": "^11.0.0-rc.390",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "hono": "^4.4.7",
    "kysely": "^0.27.3",
    "kysely-bun-worker": "^0.6.1",
