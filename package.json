{
  "name": "solid-launch",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite --host",
    "build": "tsc && vite build",
    "preview": "NODE_ENV=production bun run src/server/server.ts",
    "preview-static": "vite preview",
    "check": "tsc --noemit",
    "lint": "prettier --check .; eslint .",
    "lint:fix": "prettier --write .; eslint --fix .",
    "format": "prettier --write .",
    "db:generate": "bunx prisma generate",
    "db:migrate": "prisma migrate dev",
    "update": "bunx npm-check-updates --interactive --format group"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@felte/solid": "^1.2.14",
    "@felte/validator-zod": "^1.0.18",
    "@hono/trpc-server": "^0.3.4",
    "@kobalte/core": "^0.13.9",
    "@libsql/kysely-libsql": "^0.4.1",
    "@node-rs/argon2": "^2.0.2",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/solid-query": "^5.74.4",
    "@trpc/client": "^11.1.0",
    "@trpc/server": "^11.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "hono": "^4.7.7",
    "kysely": "^0.28.1",
    "kysely-bun-worker": "^1.2.0",
    "prisma": "^6.6.0",
    "prisma-kysely": "^1.8.0",
    "solid-js": "^1.9.5",
    "solid-sonner": "npm:@blankeos/solid-sonner",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "vike": "^0.4.228",
    "vike-metadata-solid": "^1.0.4",
    "vike-solid": "^0.7.9",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "^0.19.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@tanstack/solid-query-devtools": "^5.74.6",
    "@types/bun": "^1.2.10",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-plugin-solid": "^0.14.5",
    "postcss": "^8.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vite-plugin-solid": "^2.11.6",
    "vite-plugin-solid-svg": "^0.8.1"
  }
}
