    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@node-rs/argon2": "^2.0.2",
    "@prisma/client": "^6.1.0",
    "@tanstack/solid-query": "^5.62.8",
    "@trpc/client": "^11.0.0-rc.390",
    "@trpc/server": "^11.0.0-rc.390",
    "class-variance-authority": "^0.7.1",
