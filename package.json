    "@tanstack/solid-query-devtools": "^5.60.6",
    "@types/bun": "^1.1.13",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vike": "^0.4.203",
    "vike-solid": "^0.7.6",
